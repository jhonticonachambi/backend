    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-html-reporter",
    "report": "npm run test:coverage && node scripts/open-report.js",
    "report:serve": "npm run test:coverage && npx http-server public -p 8080 -o"
