    "dotenv": "^16.5.0",
    "express": "^4.21.1",
    "express-list-endpoints": "^7.1.1",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.7.1",
    "nodemailer": "^6.9.15",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
